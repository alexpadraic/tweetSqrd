    </div>
    <div class="collapse navbar-collapse" id="myNavbar">
      <ul class="nav navbar-nav">
      </ul>
      <ul class="nav navbar-nav navbar-right">
       <% if !current_user %>
        <li><a href="/users/new"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
        <li><a href="/sessions/new"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
         <% else %>
         <li><a href="/tweets/new"><span class="glyphicon glyphicon-plus"></span> New Tweet </a></li>
        <li><a href="/sessions/delete"><span class="glyphicon glyphicon-log-in"></span> Log out </a></li>
        <%end%>
      </ul>
    </div>
  </div>
</nav>




  <%= yield %>
